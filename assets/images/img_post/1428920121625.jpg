library(MASS)
attach(Boston)

class <-rep(0,length(crim))

i=1
for (x in crim){
	if (x > median(crim)){
		class[i]=1
	}
	i=i+1
}

Boston = data.frame(Boston,class)

###logistic 
glm.fit=glm(class~zn+indus+chas+nox+rm+age+dis+rad+tax+ptratio+black+lstat+medv,data=Boston,family=binomial)
glm.probs=predict(glm.fit,type="response")
glm.pred=rep(0,length(crim))
logistic=table(glm.pred,class)


###LDA
lda.fit=lda(class~zn+indus+chas+nox+rm+age+dis+rad+tax+ptratio+black+lstat+medv,data=Boston)
lda.pred=predict(lda.fit,Boston)
lda.class=lda.pred$class
linear=table(lda.calss,class)

